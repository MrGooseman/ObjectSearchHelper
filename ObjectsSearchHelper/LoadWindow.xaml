<Window x:Class="ObjectsSearchHelper.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObjectsSearchHelper"
        mc:Ignorable="d"
        Title="Real Estate" Height="400" Width="800" Icon="Resources/house.png" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowState="Normal" WindowStyle="None">
    <Grid x:Name="MainGrid" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label x:Name="label1" Content="Real Estate program" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
        <Label Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="16" Content="Программа, чтобы сделать жизнь проще"/>
    </Grid>
</Window>
